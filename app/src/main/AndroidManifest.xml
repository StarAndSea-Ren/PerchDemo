<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ren.yueh.perchdemo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".swustsc.SWUSTSCNewsActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".main.WelcomeActivity" />
        <activity android:name=".swustsc.NewsPageActivity" />
        <activity android:name=".main.HomeActivity"></activity>
        <activity android:name=".baijia.BaiJiaNewsListActivity"></activity>
    </application>

</manifest>